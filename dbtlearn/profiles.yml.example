# Example profiles.yml configuration
# This file should be placed in ~/.dbt/ directory
# Copy this file and update with your actual credentials

dbtlearn:
  target: dev
  outputs:
    dev:
      type: postgres
      host: localhost
      user: your_username
      password: your_password
      port: 5432
      dbname: dbtlearn
      schema: dbt_dev
      threads: 4
      keepalives_idle: 0
      
    prod:
      type: postgres
      host: localhost
      user: your_username
      password: your_password
      port: 5432
      dbname: dbtlearn
      schema: dbt_prod
      threads: 4
      keepalives_idle: 0

